{
  "name": "harperfamily-site",
  "version": "0.1.0",
  "description": "A static website with the Eleventy static site generator and Pages CMS, for deployment to Netlify.",
  "type": "module",
  "scripts": {
    "build": "npx @11ty/eleventy",
    "build-nocolor": "cross-env NODE_DISABLE_COLORS=1 npx @11ty/eleventy",
    "build-ghpages": "npx @11ty/eleventy --pathprefix=/harperfamily-site/",
    "start": "npx @11ty/eleventy --serve --quiet",
    "start-ghpages": "npx @11ty/eleventy --pathprefix=/harperfamily-site/ --serve --quiet",
    "debug": "cross-env DEBUG=Eleventy* npx @11ty/eleventy",
    "debugstart": "cross-env DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
    "benchmark": "cross-env DEBUG=Eleventy:Benchmark* npx @11ty/eleventy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HarperFamily/harperfamily-site"
  },
  "author": {
    "name": "Graham Harper",
    "email": "graham@hrpr.co",
    "url": "http://www.grahamharper.ie/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HarperFamily/harperfamily-site/issues"
  },
  "homepage": "https://github.com/HarperFamily/harperfamily-site",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-img": "^6.0.4",
    "@11ty/eleventy-navigation": "^1.0.4",
    "@11ty/eleventy-plugin-rss": "^2.0.4",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.2",
    "clean-css": "^5.3.2",
    "cross-env": "^7.0.3",
    "html-minifier-terser": "^7.2.0",
    "luxon": "^3.6.1",
    "netlify-plugin-cache": "^1.0.3",
    "prismjs": "^1.30.0",
    "uglify-js": "^3.17.4"
  },
  "dependencies": {
    "lightgallery": "^2.9.0"
  }
}
